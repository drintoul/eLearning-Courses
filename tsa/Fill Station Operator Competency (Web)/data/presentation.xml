		<slide index="7" title="Application" titleNormalized="application" src="slide8.swf" size="1672" advanceOnTime="0" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="">
			<sounds/>
