		<slide index="4" title="Communication Systems" titleNormalized="communication systems" src="slide5.swf" size="4453" advanceOnTime="0" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="">
